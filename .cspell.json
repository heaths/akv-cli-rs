{
  "version": "0.2",
  "language": "en",
  "useGitignore": true,
  "ignorePaths": [
    ".cspell.json",
    ".devcontainer/",
    ".github/",
    ".vscode/",
    "rust-toolchain.toml"
  ],
  "words": [
    "clippy",
    "debuginfo",
    "deque",
    "dotenv",
    "libc",
    "lldb",
    "repr",
    "rustlang",
    "rustup"
  ],
  "dictionaryDefinitions": [
    {
      "name": "crates",
      "words": [
        "anstream",
        "anstyle",
        "dotenvy",
        "reqwest",
        "prettytable",
        "serde",
        "timeago"
      ],
      "noSuggest": true
    }
  ],
  "overrides": [
    {
      "filename": "Cargo.toml",
      "dictionaries": [
        "crates"
      ]
    },
    {
      "filename": "**/*.md",
      "dictionaries": [
        "rust"
      ]
    },
    {
      "filename": "**/*.rs",
      "dictionaries": [
        "crates"
      ]
    }
  ]
}
